import cv2
import numpy as np
cap = cv2.VideoCapture("C:\Users\91638\Downloads\vecteezy_30-second-animation-from-30-to-0-seconds-modern-flat-design_8663528 (1).mp4")
if (cap.isOpened()== False):
print("Error opening video file")
while(cap.isOpened()):
ret, frame = cap.read()
if ret == True:
cv2.imshow('Frame', frame)
if cv2.waitKey(250) & 0xFF == ord('q'):
break

else:
break
cap.release()
cv2.destroyAllWindows()
